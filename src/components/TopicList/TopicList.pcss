.topic__list {
  display: flex;
  flex-wrap: wrap;

  &__item {
    display: flex;
    background: #ededed;
    text-decoration: none;
    padding: 5px 12px;
    margin: 0 5px 10px 0;
    border-radius: 20px;
    cursor: pointer;
    color: #000;

    &:hover {
      background: #cccccc;
    }
  }
}
